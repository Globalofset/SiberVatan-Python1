sayilar = [10,8,5,3,15]
en_buyuk = max(sayilar)
en_kucuk = min(sayilar)

print(en_buyuk)
print(en_kucuk)

sayilar.append(20)
sayilar.append(1)

yeni_en_buyuk = max(sayilar)
yeni_en_kucuk = min(sayilar)
print(sayilar)


